{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "indent":"off",
        "no-trailing-spaces":"off",
        "linebreak-style":"off",
        "max-len":"off"
    }
}
